-- Creates table for generic account context storage

CREATE TABLE IF NOT EXISTS FeeEstimation (
    currency TEXT AS CryptoCurrency PRIMARY KEY,
    low TEXT AS Value,
    economy TEXT AS Value,
    normal TEXT AS Value,
    high TEXT AS Value,
    lastCheck INTEGER AS Long
);