-- Creates table for generic logs storage

CREATE TABLE IF NOT EXISTS Logs(
    dateMillis INTEGER PRIMARY KEY NOT NULL,
    level TEXT NOT NULL,
    message TEXT NOT NULL
);

insert:
INSERT INTO Logs(dateMillis, level, message)
VALUES (?, ?, ?);

select:
SELECT * FROM Logs;

selectSince:
SELECT * FROM Logs
WHERE dateMillis > (?);

deleteSince:
DELETE FROM Logs
WHERE dateMillis > (?);

delete:
DELETE FROM Logs;

